		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		//regulator-enable-ramp-delay = <1000>;
		regulator-always-on;
		regulator-boot-on;
		gpio = <&pio PB 6 GPIO_ACTIVE_HIGH>;
		enable-active-high;
	};
