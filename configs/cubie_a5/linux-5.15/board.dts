		compatible = "regulator-fixed";
		regulator-name = "gma340-oe";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		//regulator-enable-ramp-delay = <1000>;
		regulator-always-on;
		regulator-boot-on;
		gpio = <&pio PB 7 GPIO_ACTIVE_LOW>;
	};

